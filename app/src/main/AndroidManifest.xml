<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rvbnb">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_rvbnb"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_rvbnb_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.CreateListingActivity"></activity>
        <activity android:name=".ui.ProfileActivity" />
        <activity android:name=".ui.RVOwnerActivity" />
        <activity android:name=".ui.LandownerActivity" />
        <activity
            android:name=".ui.RegisterActivity"
            android:label="Register" />
        <activity android:name=".ui.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.MainActivity" />
    </application>

</manifest>